}
.article {
  padding-top: 22rpx;
}
.order-wrap {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 2rpx;
  height: 289rpx;
  width: 750rpx;
  background-color: #FFFFFF;
}
.title {
  padding-top: 46rpx;
  font-size: 38rpx;
