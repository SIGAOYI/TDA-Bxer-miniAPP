  letter-spacing: 0.23rpx;
}
.consultbtn {
  display: flex;
  align-content: center;
  flex-direction: column;
  justify-content: center;
  box-sizing: border-box;
  margin-top: 23rpx;
  height: 88rpx;
  width: 675rpx;
  background: #FF6600;
  font-size: 38rpx;
  color: #FFFFFF;
  letter-spacing: 0.33rpx;
  border-radius: 16rpx;
}
.banner {
  margin-top: 25rpx;
  height: 246rpx;
  width: 750rpx;
  background-color: #FFFFFF;
}
.swiper {
 height: 246rpx;
 width: 750rpx;
}
.swiper image {
 height: 100%;
 width: 100%;
}
.listwrap {
  margin-top: 25rpx;
  height: 343rpx;
  width: 750rpx;
  padding-bottom: 20rpx;
  background-color: #FFFFFF;
}
.listwrapChild {
  height: 259rpx;
  width: 750rpx;
  overflow: scroll;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.listhead {
  position: relative;
  overflow-x: hidden;
  display: flex;
  flex-direction: row;
  width: 750rpx;
  height: 84rpx;
}
.listhead:after {
  content: '';
  display: block;
  height: 1rpx;
  width: 100%;
  zoom: 1;
  position: absolute;
  bottom: 0;
  left: 30rpx;
  border-bottom: 1rpx solid #EBEBEB;
}
.listtitle {
  margin-left: 36rpx;
  margin-top: 23rpx;
  font-size: 29rpx;
  color: #000000;
  letter-spacing: 0.92rpx;
}
.listtmore {
  margin-left: 516rpx;
  margin-top: 23rpx;
  font-size: 29rpx;
  color: #B9B9B9;
  letter-spacing: 0.92rpx;
}
.iconmore {
  margin-left: 3rpx;
  margin-top: 20rpx;
  width: 44rpx;
  height: 44rpx;
}
.list {
  margin-left: 37rpx;
  margin-top: 33rpx;
}
.listitem {
  margin-top: 20rpx;
  height: 48rpx;
  display: flex;
  flex-direction: row;
}
.posticon {
  margin-top: 5rpx;
  width: 30rpx;
  height: 30rpx;
}
.post {
  margin-left: 20rpx;
  font-size: 26rpx;
  color: #000000;
  letter-spacing: 0.83rpx;
}
